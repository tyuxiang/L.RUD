module check_player_module (
    input clk,  // clock
    input rst,  // reset
    output check_player,
    input answer_player,
    input player
  ) {
  new_alu alu;
  always {
    check_player=hx;
    alu.firstNumber=player;
    alu.secondNumber=answer_player;
    alu.alufn=b110011;
    check_player=alu.answer;
  }
}